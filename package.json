{
  "name": "sparky",
  "version": "1.0.0",
  "description": "This Discord bot is designed to be an easy and customizable solution for your servers. Ideal for groups of friends, the bot offers a variety of fun and useful commands that enhance chat experience and community interaction.",
  "main": "dist/bot.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/bot.ts",
    "start": "npm run build && node .",
    "build": "tsc",
    "clean": "tsc --build --clean",
    "format": "prettier --write .",
    "docs": "typedoc --entryPointStrategy expand src --out docs"
  },
  "keywords": [],
  "author": "Sebana02",
  "license": "MIT",
  "dependencies": {
    "@discord-player/extractor": "^4.5.1",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "discord-player": "^6.7.1",
    "discord-player-youtubei": "^1.3.2",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "mediaplex": "^0.0.9"
  },
  "bugs": {
    "url": "https://github.com/Sebana02/sparky/issues"
  },
  "homepage": "https://github.com/Sebana02/sparky#readme",
  "devDependencies": {
    "@types/node": "^22.8.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3"
  }
